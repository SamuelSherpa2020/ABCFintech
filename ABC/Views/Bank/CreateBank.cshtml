@model ABC.Dtos.BankDto.CreateBankDto

<h2>Create Bank</h2>

<form asp-action="CreateBank" method="post">
    <div class="form-group" style="width:250px; margin-bottom:20px;">
        <label asp-for="BankName" class="control-label"></label>
        <input asp-for="BankName" class="form-control" />
        <span asp-validation-for="BankName" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
</form>
